<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="utils.com.futpro"

    >
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".NewUserActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity

            android:name=".MatchActivity">

        </activity>

        <activity

            android:name="profile.SessionActivity"
            android:screenOrientation="portrait"
            >

        </activity>

        <activity

            android:name=".PlayerCardRegistration">

        </activity>



        <activity

            android:name=".ProfileActivity"
            android:screenOrientation="portrait"
            >

        </activity>

        <activity

            android:name=".QueueActivity"
            android:screenOrientation="portrait"
            >

        </activity>

        <activity

            android:name=".ChallengeActivity"
            android:screenOrientation="portrait"
            >

        </activity>


        <activity

            android:name=".FUTSearchActivty"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan|adjustResize"
            >

        </activity>

        <activity

            android:name=".TeamEditActivty"
            android:screenOrientation="portrait"
            >

        </activity>


        <activity

            android:name=".OpponentTeamActivity"
            android:screenOrientation="portrait"
            >

        </activity>

        <activity

            android:name=".TestActivity"
            android:screenOrientation="portrait"
            >

        </activity>


        <service android:name="profile.services" />

    </application>

</manifest>